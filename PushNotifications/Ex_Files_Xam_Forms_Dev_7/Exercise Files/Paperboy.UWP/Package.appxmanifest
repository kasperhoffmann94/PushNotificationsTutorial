<?xml version="1.0" encoding="utf-8"?>
<Package xmlns="http://schemas.microsoft.com/appx/manifest/foundation/windows10" xmlns:mp="http://schemas.microsoft.com/appx/2014/phone/manifest" xmlns:uap="http://schemas.microsoft.com/appx/manifest/uap/windows10" IgnorableNamespaces="uap mp">
  <Identity Name="LiquidDaffodil.PaperboyforWindows" Publisher="CN=8934FEF0-6C8F-4AAD-89D6-5428965707BE" Version="1.1.0.0" />
  <mp:PhoneIdentity PhoneProductId="c8236c33-84d0-40ba-9e3c-c546274450ba" PhonePublisherId="00000000-0000-0000-0000-000000000000" />
  <Properties>
    <DisplayName>Paperboy for Windows</DisplayName>
    <PublisherDisplayName>Liquid Daffodil</PublisherDisplayName>
    <Logo>Assets\pb_store.png</Logo>
  </Properties>
  <Dependencies>
    <TargetDeviceFamily Name="Windows.Universal" MinVersion="10.0.0.0" MaxVersionTested="10.0.0.0" />
  </Dependencies>
  <Resources>
    <Resource Language="x-generate" />
  </Resources>
  <Applications>
